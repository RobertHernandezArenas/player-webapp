		"eslint": "^8.13.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-vue": "^8.6.0",
		"prettier": "2.6.2",
		"vite": "^2.9.2"
	},
	"extends": [
		"plugin:vue/recommended",
		"eslint:recommended",
		"prettier",
		"prettier/vue"
