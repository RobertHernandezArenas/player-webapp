{
	"name": "frontsite",
	"private": true,
	"version": "0.0.0",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview",
		"lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
		"format": "prettier .  --write"
	},
	"dependencies": {
		"intl-tel-input": "^17.0.16",
		"vite-plugin-eslint": "^1.4.0",
